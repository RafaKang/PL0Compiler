procedure p(a: integer);
  procedure q;
    procedure r;
    begin
      write(a)
    end;
  begin
    r
  end;
begin
  q
end;
function f(a: integer): integer;
  procedure q(b: integer);
    procedure r(c: integer);
    begin
      f := a+b+c
    end;
  begin
    p(b);
    r(b+b)
  end;
begin
  p(a);
  q(a+a)
end;
begin
  write(f(1))
end.
