begin
  //if 1<2 and (1>2 or 2<3) then write("good")
  if 1<2 and 1<2 then write("true and true");
  if 1<2 and 1>2 then write("true and false");
  if 1>2 and 1<2 then write("false and true");
  if 1>2 and 1>2 then write("false and false");
  if 1<2 or 1<2 then write("true or true");
  if 1<2 or 1>2 then write("true or false");
  if 1>2 or 1<2 then write("false or true");
  if 1>2 or 1>2 then write("false or false");
end.
